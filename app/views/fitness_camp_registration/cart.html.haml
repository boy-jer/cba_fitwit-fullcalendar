- title "View Cart: Registration Step 2 of 6"

- present @cart do |cart_presenter|

  - content_for :right_column do
    = cart_presenter.show_cart

  #cart_information_region
    #vet_status_changer
      = cart_presenter.fit_wit_history(@user)
    #membership_region
      = cart_presenter.membership_info(@user)

  #cart_contents_region
    = cart_presenter.show_camps(@cart.items)
    = link_to 'Proceed to health consent', 'joo', :class => 'small-btn glass-btn'

