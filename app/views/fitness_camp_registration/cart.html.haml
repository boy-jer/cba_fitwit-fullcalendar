- title "View Cart: Registration Step 2 of 7"

- present @cart do |cart_presenter|

  #cart_information_region
    #vet_status_changer
      = cart_presenter.fit_wit_history(@user)
    #membership_region
      = cart_presenter.membership_info(@user)

  #cart_contents_region
    = cart_presenter.show_camps(@cart.items)
    .form-actions
      = button_to 'Proceed to Update Personal Information', fitness_camp_registration_update_profile_path, method: :get, class: 'btn'
